{% extends "default/bars.html.twig" %}

{% block contenpage %}
	<div class="">
		<div class="page-title">
			<div class="title_left">
				<h5>
					Iconos
				</h5>
			</div>
		</div>

		<div class="clearfix"></div>

		<div class="row">
			<div class="col-md-12">
				<div class="x_panel">
					<div class="x_title">
						<h2>
							Lista de iconos</h2>
						<div class="clearfix"></div>
					</div>
					<div class="x_content">
						<div class="row">
							{% for fa in fas %}
								<div class="col-1 mb-2"><i class="text-center fa {{ fa }}" style="font-size: 30px;"></i></div>
								<div class="col-2 mb-2">{{ fa }}</div>
							{% endfor %}
							<table class="display table table-striped table-bordered dataminetables">
								<thead>
									<th>Icono</th>
									<th>Clase</th>
								</thead>
								<tbody>
									{% for fa in fas %}
									<tr>
										<td><i class="text-center fa {{ fa }}" style="font-size: 30px;"></i></td>
										<td>fa {{ fa }}</td>
									</tr>
								{% endfor %}
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
